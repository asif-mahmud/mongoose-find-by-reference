{
  "name": "@gswag/mongoose-find-by-reference",
  "author": "Asif Mahmud Shimon",
  "homepage": "https://github.com/cheezone/mongoose-find-by-reference",
  "version": "1.0.8",
  "description": "Make Mongoose support finding in reference fields.",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "private": false,
  "scripts": {
    "start": "npx ts-node src/main.ts",
    "build": "rimraf ./dist && tsc",
    "release": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheezone/mongoose-find-by-reference.git"
  },
  "bugs": {
    "url": "https://github.com/cheezone/mongoose-find-by-reference/issues"
  },
  "keywords": [
    "mongoose",
    "reference",
    "ref",
    "objectid",
    "mongodb"
  ],
  "license": "Apache 2.0",
  "peerDependencies": {
    "mongoose": "6.2.1"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.8.2",
    "tsup": "^6.1.3",
    "typescript": "<4.5"
  },
  "dependencies": {
    "mocha": "^10.0.0"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "minify": true
  }
}
